{
    "name": "carropublic/follow",
    "description": ":Followable polymorph",
    "license": "MIT",
    "authors": [
        {
            "name": "Sithu Aung",
            "email": "sithu.aung@carro.co",
            "homepage": "https://carro.sg"
        }
    ],
    "homepage": "https://github.com/carropublic/follow",
    "keywords": ["Laravel", "Follow"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CarroPublic\\Follow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarroPublic\\Follow\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarroPublic\\Follow\\FollowServiceProvider"
            ],
            "aliases": {
                "Follow": "CarroPublic\\Follow\\Facades\\Follow"
            }
        }
    }
}
